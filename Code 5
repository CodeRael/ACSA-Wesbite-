
      function printBtn() {
        for (var gridY = 0; gridY < tileCount; gridY++) {
          for (var gridX = 0; gridX < tileCount; gridX++) {
             var btn = document.createElement("button");
             var t = document.createTextNode(gridY, gridX[i]);
             btn.appendChild(t);
             document.body.appendChild(btn);
          }
      }